{
  "version": 2,
  "builds": [
    {
      "src": "*.{css,js,png,jpg,jpeg,gif,svg,ico,woff,woff2,ttf,eot,html,xml,txt}",
      "use": "@vercel/static"
    },
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  "headers": [
    {
      "source": "/(.*\\.(css|js|png|jpg|jpeg|gif|svg|ico|woff|woff2|ttf|eot|html|xml|txt))$",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/server.js"
    }
  ],
  "redirects": [
    {
      "source": "/www.lugvia.com/(.*)",
      "destination": "https://lugvia.com/$1",
      "permanent": true
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}