{
  "version": 2,
  "builds": [
    {
      "src": "*.{css,js,png,jpg,jpeg,gif,svg,ico,webp,woff,woff2,ttf,eot,html,xml,txt}",
      "use": "@vercel/static"
    },
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/server.js"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}